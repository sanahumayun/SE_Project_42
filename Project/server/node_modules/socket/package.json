{
  "name": "socket",
  "version": "0.14.77",
  "description": "CLI tool for Socket.dev",
  "homepage": "https://github.com/SocketDev/socket-cli",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SocketDev/socket-cli.git"
  },
  "author": {
    "name": "Socket Inc",
    "email": "eng@socket.dev",
    "url": "https://socket.dev"
  },
  "bin": {
    "socket": "bin/cli.js",
    "socket-npm": "bin/npm-cli.js",
    "socket-npx": "bin/npx-cli.js"
  },
  "exports": {
    "./bin/cli.js": {
      "types": "./dist/module-sync/cli.d.ts",
      "default": "./dist/cli.js"
    },
    "./bin/npm-cli.js": {
      "types": "./dist/module-sync/npm-cli.d.ts",
      "default": "./dist/npm-cli.js"
    },
    "./bin/npx-cli.js": {
      "types": "./dist/module-sync/npx-cli.d.ts",
      "default": "./dist/npx-cli.js"
    },
    "./package.json": "./package.json",
    "./translations.json": "./translations.json"
  },
  "scripts": {
    "build": "run-p -c --aggregate-output build:*",
    "build:dist": "del-cli 'dist' && rollup -c .config/rollup.dist.config.mjs && npm run lint:dist:fix",
    "build:test-dist": "del-cli 'test/dist' && cross-env VITEST=1 rollup -c .config/rollup.test.config.mjs",
    "check": "run-p -c --aggregate-output check:*",
    "check:lint": "eslint --report-unused-disable-directives .",
    "check:lint:dist": "cross-env LINT_DIST=1 eslint --report-unused-disable-directives --quiet ./dist",
    "check:tsc": "tsc",
    "coverage": "run-s coverage:*",
    "coverage:test": "run-s test:prepare test:unit:coverage",
    "coverage:type": "type-coverage --detail",
    "clean": "run-p --aggregate-output clean:*",
    "clean:dist": "del-cli 'dist' 'test/dist'",
    "clean:node_modules": "del-cli '**/node_modules'",
    "fix": "npm run lint:fix",
    "knip:dependencies": "knip --dependencies",
    "knip:exports": "knip --include exports,duplicates",
    "lint": "oxlint -c=./.oxlintrc.json --ignore-path=./.oxlintignore --tsconfig=./tsconfig.json .",
    "lint:dist": "oxlint -c=./.oxlintrc.json --ignore-path=./.oxlintignore --tsconfig=./tsconfig.json ./dist",
    "lint:dist:fix": "cross-env LINT_DIST=1 npm run lint:dist -- --silent --fix | dev-null && run-s lint:dist:fix:*",
    "lint:dist:fix:fast": "biome format --write ./dist",
    "lint:dist:fix:slow": "npm run check:lint:dist -- --fix | dev-null",
    "lint:fix": "npm run lint -- --quiet --fix && run-s lint:fix:*",
    "lint:fix:fast": "biome format --write .",
    "lint:fix:slow": "npm run check:lint -- --fix",
    "lint-staged": "lint-staged",
    "precommit": "lint-staged",
    "prepare": "husky && custompatch",
    "bs": "npm run build:dist; npm exec socket --",
    "s": "npm exec socket --",
    "test": "run-s check test:*",
    "test:prepare": "cross-env VITEST=1 npm run build && del-cli 'test/**/node_modules'",
    "test:unit": "vitest --run",
    "test:unit:update": "vitest --run --update",
    "test:unit:coverage": "vitest run --coverage",
    "test-ci": "run-s test:*",
    "testu": "cross-env SOCKET_CLI_NO_API_TOKEN=1 run-s test:prepare; npm run test:unit:update --",
    "testuf": "cross-env SOCKET_CLI_NO_API_TOKEN=1 npm run test:unit:update --",
    "update": "run-p --aggregate-output update:**",
    "update:deps": "npx --yes npm-check-updates"
  },
  "dependencies": {
    "@apideck/better-ajv-errors": "0.3.6",
    "@octokit/rest": "^21.1.1",
    "@pnpm/lockfile.detect-dep-types": "^1001.0.6",
    "@pnpm/lockfile.fs": "^1001.1.8",
    "@pnpm/logger": "^1000.0.0",
    "@socketregistry/hyrious__bun.lockb": "1.0.17",
    "@socketregistry/indent-string": "1.0.12",
    "@socketregistry/is-interactive": "1.0.5",
    "@socketregistry/packageurl-js": "1.0.5",
    "@socketsecurity/config": "2.1.3",
    "@socketsecurity/registry": "1.0.137",
    "@socketsecurity/sdk": "1.4.24",
    "browserslist": "4.24.4",
    "chalk-table": "1.0.2",
    "cmd-shim": "7.0.0",
    "common-tags": "1.8.2",
    "fast-content-type-parse": "^2.0.0",
    "hpagent": "1.2.0",
    "ignore": "7.0.3",
    "meow": "13.2.0",
    "micromatch": "4.0.8",
    "npm-package-arg": "12.0.2",
    "open": "10.1.0",
    "pony-cause": "2.1.11",
    "semver": "7.7.1",
    "terminal-link": "2.1.1",
    "tiny-updater": "3.5.3",
    "tinyglobby": "0.2.12",
    "which": "5.0.0",
    "yaml": "2.7.1",
    "yargs-parser": "21.1.1",
    "yoctocolors-cjs": "2.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/plugin-proposal-export-default-from": "^7.25.9",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-export-namespace-from": "^7.25.9",
    "@babel/plugin-transform-runtime": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-typescript": "^7.27.0",
    "@babel/runtime": "^7.27.0",
    "@biomejs/biome": "^1.9.4",
    "@cyclonedx/cdxgen": "^11.2.3",
    "@eslint/compat": "^1.2.8",
    "@eslint/js": "^9.23.0",
    "@rollup/plugin-commonjs": "28.0.3",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/pluginutils": "^5.1.4",
    "@types/blessed": "0.1.25",
    "@types/cmd-shim": "^5.0.2",
    "@types/common-tags": "^1.8.4",
    "@types/micromatch": "^4.0.9",
    "@types/mock-fs": "^4.13.4",
    "@types/node": "^22.14.0",
    "@types/npmcli__arborist": "^6.3.0",
    "@types/proc-log": "^3.0.4",
    "@types/semver": "^7.7.0",
    "@types/which": "^3.0.4",
    "@types/yargs-parser": "^21.0.3",
    "@typescript-eslint/parser": "^8.29.0",
    "@vitest/coverage-v8": "3.1.1",
    "blessed": "0.1.81",
    "blessed-contrib": "4.11.0",
    "cross-env": "^7.0.3",
    "custompatch": "^1.1.4",
    "del-cli": "^6.0.0",
    "dev-null-cli": "^2.0.0",
    "eslint": "^9.23.0",
    "eslint-import-resolver-typescript": "^4.3.1",
    "eslint-plugin-import-x": "^4.10.0",
    "eslint-plugin-n": "^17.17.0",
    "eslint-plugin-sort-destructure-keys": "^2.0.0",
    "eslint-plugin-unicorn": "^56.0.1",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "knip": "^5.46.5",
    "lint-staged": "^15.5.0",
    "magic-string": "^0.30.17",
    "mock-fs": "^5.5.0",
    "nock": "^14.0.2",
    "npm-run-all2": "^7.0.2",
    "oxlint": "0.16.4",
    "read-package-up": "^11.0.0",
    "rollup": "4.39.0",
    "rollup-plugin-ts": "^3.4.5",
    "synp": "^1.9.14",
    "type-coverage": "^2.29.7",
    "typescript": "5.4.5",
    "typescript-eslint": "^8.29.0",
    "unplugin-purge-polyfills": "^0.0.7",
    "vitest": "3.1.1"
  },
  "overrides": {
    "aggregate-error": "npm:@socketregistry/aggregate-error@^1",
    "es-define-property": "npm:@socketregistry/es-define-property@^1",
    "function-bind": "npm:@socketregistry/function-bind@^1",
    "globalthis": "npm:@socketregistry/globalthis@^1",
    "gopd": "npm:@socketregistry/gopd@^1",
    "has-property-descriptors": "npm:@socketregistry/has-property-descriptors@^1",
    "has-proto": "npm:@socketregistry/has-proto@^1",
    "has-symbols": "npm:@socketregistry/has-symbols@^1",
    "hasown": "npm:@socketregistry/hasown@^1",
    "indent-string": "npm:@socketregistry/indent-string@^1",
    "is-core-module": "npm:@socketregistry/is-core-module@^1",
    "isarray": "npm:@socketregistry/isarray@^1",
    "npm-package-arg": "$npm-package-arg",
    "packageurl-js": "npm:@socketregistry/packageurl-js@^1",
    "path-parse": "npm:@socketregistry/path-parse@^1",
    "safe-buffer": "npm:@socketregistry/safe-buffer@^1",
    "safer-buffer": "npm:@socketregistry/safer-buffer@^1",
    "semver": "$semver",
    "set-function-length": "npm:@socketregistry/set-function-length@^1",
    "shell-quote": "npm:shell-quote@^1",
    "side-channel": "npm:@socketregistry/side-channel@^1",
    "tiny-colors": "$yoctocolors-cjs",
    "typedarray": "npm:@socketregistry/typedarray@^1",
    "undici": "^6.21.1",
    "vite": "^6.2.5",
    "xml2js": "^0.5.0",
    "yaml": "2.7.1"
  },
  "engines": {
    "node": "^18.20.7 || ^20.18.3 || >=22.14.0"
  },
  "files": [
    "bin/**",
    "dist/**",
    "shadow-bin/**",
    "translations.json"
  ],
  "lint-staged": {
    "*.{cjs,cts,js,json,md,mjs,mts,ts}": [
      "npm run lint -- --fix",
      "npm run lint:fix:fast -- --no-errors-on-unmatched --files-ignore-unknown=true --colors=off"
    ]
  },
  "typeCoverage": {
    "cache": true,
    "atLeast": 95,
    "ignoreAsAssertion": true,
    "ignoreCatch": true,
    "ignoreEmptyType": true,
    "ignore-non-null-assertion": true,
    "ignore-type-assertion": true,
    "ignore-files": "test/*",
    "strict": true
  }
}
